{
  "name": "d2l-telemetry-browser-client",
  "description": "Client for sending telemetry from the browser to a telemetry service",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test nyc mocha test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/d2l-telemetry-browser-client.git"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-config-brightspace": "^0.3.1",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
